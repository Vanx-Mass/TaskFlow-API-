      "nome": "Testar criação de tarefa",
      "codigo": [
        "print(\"\\n📝 Testando criação de tarefa...\")",
        "usuario_id = response.json()[\"id\"]",
        "payload = {",
        "    \"titulo\": \"Estudar Python\",",
        "    \"descricao\": \"Completar curso de FastAPI\",",
        "    \"prioridade\": \"alta\",",
        "    \"usuario_id\": usuario_id",

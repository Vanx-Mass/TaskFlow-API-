    "print(\"✅ Usuário encontrado:\")\n",
    "print_json(usuario_buscado)\n",
    "\n",
    "# Buscar tarefa específica\n",
    "response = requests.get(f\"{BASE_URL}/tarefas/{tarefa1['id']}\")\n",

    "import requests\n",
    "import json\n",
    "from uuid import UUID\n",
    "\n",
    "BASE_URL = \"http://localhost:8000\"\n",
    "\n",
    "def print_json(data):\n",
    "    \"\"\"Função para imprimir JSON formatado\"\"\"\n",
    "    print(json.dumps(data, indent=2, ensure_ascii=False))"
   ]
